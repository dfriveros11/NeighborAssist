{
  "name": "neighborassist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "cd front && yarn install",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd front && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "cd front && yarn install && yarn run build"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.17.3",
    "google-map-react": "^1.1.7",
    "http-errors": "~1.6.3",
    "mongodb": "^3.5.5",
    "morgan": "~1.9.1",
    "mongoose": "^5.9.7",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "react-google-maps": "^9.4.5"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  }
